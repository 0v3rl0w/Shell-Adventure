; Objectif de création de dossier / fichier


FUN out
	DONE dossier
	SAY "Passez une bonne journée !"
	WAIT 2s THEN EXIT
END FUN

FUN finish
	DONE fichier
	SAY "Bien joué :p"
	OBJECTIF dossier "Crée un dossier garage"
	WAIT DIR "garage" THEN CALL out
END FUN

OBJECTIF fichier "Crée un fichier mamie"
WAIT FILE "mamie" THEN CALL finish